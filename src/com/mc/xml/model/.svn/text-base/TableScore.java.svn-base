package com.mc.xml.model;

public class TableScore {

	private String xn;//学年
	private String xq;//学期
	private String kcmc;//课程
	private String kcxz;//课程类型
	private String xf;//学分
	private String pscj;//平时成绩
	private String qmcj;//期末成绩
	private String cj;//最终成绩
	private String xymc;//学院
	
	public String getXymc() {
		return xymc;
	}
	public void setXymc(String xymc) {
		this.xymc = xymc;
	}
	public String getXn() {
		return xn;
	}
	public void setXn(String xn) {
		this.xn = xn;
	}
	public String getXq() {
		return xq;
	}
	public void setXq(String xq) {
		this.xq = xq;
	}
	public String getKcmc() {
		return kcmc;
	}
	public void setKcmc(String kcmc) {
		this.kcmc = kcmc;
	}
	public String getKcxz() {
		return kcxz;
	}
	public void setKcxz(String kcxz) {
		this.kcxz = kcxz;
	}
	public String getXf() {
		return xf;
	}
	public void setXf(String xf) {
		this.xf = xf;
	}
	public String getPscj() {
		return pscj;
	}
	public void setPscj(String pscj) {
		this.pscj = pscj;
	}
	public String getQmcj() {
		return qmcj;
	}
	public void setQmcj(String qmcj) {
		this.qmcj = qmcj;
	}
	public String getCj() {
		return cj;
	}
	public void setCj(String cj) {
		this.cj = cj;
	}
}
